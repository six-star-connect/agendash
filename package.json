{
  "name": "agendash-aws",
  "version": "3.1.8",
  "description": "A modern dashboard for Agenda.js with Pagination and Search capabilities with AWS support",
  "main": "app.js",
  "bin": "bin/agendash-standalone.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "run-p lint ava",
    "lint": "eslint ./",
    "ava": "ava -v -c 1"
  },
  "keywords": [
    "agenda",
    "agendash",
    "dashboard",
    "job-queues",
    "aws",
    "documentdb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/six-star-connect/agendash.git"
  },
  "bugs": {
    "url": "https://github.com/six-star-connect/agendash/issues"
  },
  "homepage": "https://github.com/six-star-connect/agendash#readme",
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "public",
    "app.js",
    "*.png",
    "Dockerfile",
    "entrypoint.sh"
  ],
  "dependencies": {
    "agenda": "^4.3.0",
    "body-parser": "^1.20.0",
    "commander": "^9.4.0",
    "express": "^4.18.1",
    "mongodb": "*",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@hapi/hapi": "^20.2.2",
    "@hapi/inert": "^7.0.0",
    "ava": "4.3.1",
    "eslint": "8.20.0",
    "fastify": "^4.3.0",
    "fastify-static": "^4.7.0",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-router": "12.0.0",
    "koa-static": "5.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "supertest": "^6.2.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended"
  },
  "eslintIgnore": [
    "public"
  ]
}
